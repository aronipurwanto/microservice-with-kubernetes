{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Consul": {
    "Url": "http://localhost:8500"
  },
  "Service": {
    "Port": 5004,
    "Name": "inventory-service"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=localhost;Database=InventoryService;Trusted_Connection=true;TrustServerCertificate=true;"
  },
  "Inventory": {
    "LowStockThreshold": 10,
    "AutoReorderEnabled": true
  },
  "Urls": "http://0.0.0.0:5004"
}
